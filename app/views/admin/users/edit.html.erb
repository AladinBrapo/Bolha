<h1>Edit User</h1>

<%= form_for [:admin, @user] do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email, disabled: true %>
  </div>

  <div>
    <%= f.label :admin %>
    <%= f.check_box :admin %>
  </div>

  <div>
    <%= f.submit "Update User" %>
  </div>
<% end %>

<%= link_to "Back to Users", admin_users_path %>
